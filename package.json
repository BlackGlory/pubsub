{
  "private": true,
  "name": "pubsub",
  "version": "1.0.0",
  "main": "lib/index.js",
  "repository": "git@github.com:BlackGlory/pubsub.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "cross-env PUBSUB_HTTP2=true nodemon",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "start": "node lib/index.js",
    "start:pm2": "pm2 start lib/index.js --name 'pubsub' --wait-ready",
    "start:http2": "cross-env PUBSUB_HTTP2=true node lib/index.js",
    "docker:build": "docker build --tag pubsub .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.0",
    "@blackglory/jest-matchers": "^0.1.21",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/app-root-path": "^1.2.4",
    "@types/better-sqlite3": "^7.4.1",
    "@types/jest": "^27.0.3",
    "@types/jest-json-schema": "^2.1.3",
    "@types/node": "^14.14.44",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "extra-fetch": "^1.8.2",
    "extra-request": "^3.0.2",
    "extra-response": "^0.3.8",
    "husky": "4",
    "jest": "^27.3.1",
    "jest-extended": "^1.1.0",
    "jest-json-schema": "^5.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.2.0",
    "pm2": "^5.1.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.7",
    "@blackglory/errors": "^1.1.2",
    "@blackglory/go": "^0.1.0",
    "@blackglory/structures": "^0.2.6",
    "@blackglory/types": "^0.5.2",
    "@blackglory/wait-for": "^0.3.4",
    "ajv": "^8.8.1",
    "app-root-path": "^3.0.0",
    "better-sqlite3": "^7.4.5",
    "dotenv": "^10.0.0",
    "extra-filesystem": "^0.3.13",
    "extra-generator": "^0.2.14",
    "extra-promise": "^0.19.1",
    "extra-tags": "^0.2.2",
    "extra-timers": "^0.2.2",
    "fastify": "^3.24.0",
    "fastify-bearer-auth": "^6.0.0",
    "fastify-cors": "^6.0.2",
    "fastify-metrics": "^8.0.0",
    "fastify-plugin": "^3.0.0",
    "justypes": "^0.1.6",
    "migrations-file": "^0.1.10",
    "prom-client": "^14.0.1",
    "return-style": "^0.12.4",
    "rxjs": "^7.4.0",
    "value-getter": "^0.2.1",
    "ws": "^8.2.3"
  }
}
